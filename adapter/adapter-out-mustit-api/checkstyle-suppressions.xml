<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <!-- DTO는 record로 생성되므로 Javadoc 완화 -->
    <suppress checks="JavadocMethod" files="TokenRequest\.java"/>
    <suppress checks="JavadocMethod" files="TokenApiResponse\.java"/>
    <suppress checks="MissingJavadocMethod" files="TokenRequest\.java"/>
    <suppress checks="MissingJavadocMethod" files="TokenApiResponse\.java"/>
    <suppress checks="JavadocType" files="TokenRequest\.java"/>
    <suppress checks="JavadocType" files="TokenApiResponse\.java"/>
</suppressions>
