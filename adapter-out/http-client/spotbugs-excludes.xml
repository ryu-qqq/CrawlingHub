<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- HTTP Client Adapter SpotBugs Exclusions -->

    <!-- RestTemplate Response DTOs (External API 응답) -->
    <Match>
        <Class name="~.*Response$"/>
        <Bug pattern="URF_UNREAD_FIELD"/>
    </Match>

    <!-- Configuration Classes -->
    <Match>
        <Class name="~.*Config$"/>
        <Bug pattern="UWF_UNWRITTEN_FIELD"/>
    </Match>
</FindBugsFilter>
